//蟻本　pi(x)
ll prime[10000000];
bool is_prime[10000001];
ll sieve(ll n){
    ll p=0;
    rep(i,n+1)is_prime[i]=true;
    is_prime[0]=is_prime[1]=false;
    for(ll i=2;i<=n;i++){
        if(is_prime[i]){
            prime[p++]=i;
            for(ll j=2*i;j<=n;j+=i)is_prime[j]=false;
        }
    }
    return p;
}
int main(){
    cout<<sieve()<<endl;
}


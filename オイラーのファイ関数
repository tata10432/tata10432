vector<P>prime(ll N){vector<P>res;
    for(ll a=2;a*a<=N;++a){if(N%a!=0)continue;ll r=0;
        while(N%a==0){++r; N/=a;} res.push_back({a,r});}
    if(N!=1)res.push_back({N,1});return res;}
ll phi(ll N){const auto &pf =prime(N);ll res =N;
    for(auto p:pf){res *=(p.first-1); res/=p.first;}return res;}
